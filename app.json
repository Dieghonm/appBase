{
  "expo": {
    "name": "login-base",
    "slug": "login-base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./src/assets/Logo.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
